{
  "manifest_version": 3,
  "name": "Cookie Viewer",
  "version": "1.0",
  "description": "Displays cookies from the current site",
  "permissions": ["cookies", "activeTab", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.png"
  }
}
